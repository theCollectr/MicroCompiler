PROGRAM test4
BEGIN

	FLOAT x,y,z,t;
	STRING newline := "\n";

	FUNCTION VOID main()
	BEGIN
		x := 1.0;
		y := 2.0;
		z := 3.14159;
		WRITE (x, newline);
		WRITE (z, newline);
		WRITE (y, newline);
		x := z/2.0;
		y := z/y;
		WRITE (x, newline);
		WRITE (y, newline);
		t := (x+y+z)/z;
		WRITE (t, newline);
		t := t*t+1;
		WRITE (t, newline);
		t := (t+z+t+t/2.0+z/4.0+z/5.0+z/6.0+z/7.0);
		WRITE (t, newline);
	END	
END
