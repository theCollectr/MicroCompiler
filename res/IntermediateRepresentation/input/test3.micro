PROGRAM test3
BEGIN

	INT a,b,c,d;
	STRING newline := "\n";

	FUNCTION VOID main()
	BEGIN
		a := 1;
		b := 2;
		c := 10;
		d := 20;

		WRITE (a, newline);
		WRITE (b, newline);
		WRITE (c, newline);
		WRITE (d, newline);
		a := a + b;
		WRITE (a, newline);
		b := a * c;
		WRITE (b, newline);
		c := 0 - a + b;
		WRITE (c, newline);
		d := 0 - d;
		WRITE (d, newline);
		a := (a+b)*(d+c)-(a+b+c+d)/a;
		WRITE (a, newline);
		a := a + 10;
		WRITE (a, newline);
		b := b + a + 10;
		WRITE (b, newline);
		c := 0 - 10;
		WRITE (c, newline);
	END	
END
